{
  "name": "ts-er-generator",
  "version": "0.1.0",
  "description": "Generate ER diagrams from TypeScript definitions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ts-er": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:web": "vite build",
    "dev": "tsc --watch",
    "dev:web": "vite",
    "test": "vitest",
    "test:run": "vitest run",
    "benchmark": "tsx benchmarks/compare.ts",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "typescript",
    "er-diagram",
    "mermaid",
    "code-generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "commander": "^12.0.0",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "mermaid": "^10.6.0",
    "open": "^10.0.0",
    "typescript": "^5.3.3",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.7.0",
    "vite": "^5.0.0",
    "vitest": "^1.1.0"
  }
}
